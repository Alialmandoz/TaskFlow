# .env.example
# This file provides an example of environment variables that can be set for this project.
# Copy this file to .env and fill in your actual values.
# Do NOT commit the .env file to version control.

# Django settings
# DJANGO_DEBUG=True
# DJANGO_SECRET_KEY=your_secret_key_here  # Make sure to set a strong, unique key

# Allowed hosts (comma-separated if multiple, or a single host)
# DJANGO_ALLOWED_HOST=yourdomain.com,anotherdomain.com
# For development with DEBUG=True, this can often be left unset as ALLOWED_HOSTS defaults to ['127.0.0.1', 'localhost']
